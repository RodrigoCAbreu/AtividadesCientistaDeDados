setwd("~/ProjetosCientistaDeDados/BigDataRAzure/Cap11")
x <- seq(0, 100)
y <- 2 * x + 35
x
y
y1 <- y + rnorm(101, 0, 50)
y1
hist(y1)
plot(x = x, y = y1, main = "Relacionamento X e Y1", xlab = "X", ylab = "Y1")
?lm
regressao <- lm(y1 ~ x)
regressao
summary(regressao)
anova(regressao)
abline(regressao)
plot(x, y1, main = "Relacionamento X e Y1", xlab = "X", ylab = "Y1")
a <- regressao$coefficients[1]
b <- regressao$coefficients[2]
a
b
y2 <- a + b*x
abline(regressao)
lines(x,y2,lwd = 2)
y3 <- (y2[51]-50*(b-1))+(b-1)*x
y3
lines(x,y3,lty=3)
y4 <- (y2[51]-50*(b+1))+(b+1)*x
lines(x,y4,lty=3)
y5 <- (y2[51]-50*(b+2))+(b+2)*x
lines(x,y5,lty=3)
