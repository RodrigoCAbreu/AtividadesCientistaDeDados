grafh <- function(n,p) {
x <- dbinom(0:n, size = n, prob = p)
barplot(x, ylim = 0,0.4, names.arg = 0:n)
main = sprintf(paste("Binomial Distribution", n,p,sep = ","))
}
grafh(10,0.8)
